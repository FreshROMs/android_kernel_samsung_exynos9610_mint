				};
			};

			torch {
				torch1-gpio = "gpg3-1";
				flash1-gpio = "gpg3-0";

				status = "okay";

				leds1 {
					ledname = "leds-sec1";
					id = <0>;
					brightness = <0xC>; /* (n+1)x100 : 1300mA */
					timeout = <0x7>; /* (n+1)x62.6 : 500ms */
