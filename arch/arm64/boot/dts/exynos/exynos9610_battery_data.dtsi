&pinctrl_0 {
	usbpd_irq: usbpd-irq {
		samsung,pins = "gpa2-3";
		samsung,pin-function = <0x0f>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&pinctrl_4 {
	chg_en: chg-en {
		samsung,pins = "gpg2-3";
		samsung,pin-function = <1>;
		samsung,pin-pud = <1>;
		samsung,pin-drv = <0>;
	};
};

&i2c_0 {
	i2c-gpio,delay-us = <5>;
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	usbpd-5713@33 {
		compatible = "sm5713-usbpd,i2c";
		reg = <0x33>;
		pinctrl-names = "default";
		vconn-en;
		support_pd_role_swap;
		pinctrl-0 = <&usbpd_irq>;
		usbpd,usbpd_int = <&gpa2 3 0>;
	};
};
