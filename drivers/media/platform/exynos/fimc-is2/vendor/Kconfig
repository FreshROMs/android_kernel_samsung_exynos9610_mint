menu "Vendor Feature Setting"
	depends on EXYNOS_FIMC_IS

choice
	prompt "Use vender features"
	depends on EXYNOS_FIMC_IS
	default VENDER_DEFAULT
	help
	  Use the vender specific features.

config VENDER_DEFAULT
        bool "Default"
config VENDER_MCD
        bool "MCD"
config VENDER_MCD_V2
        bool "MCD_V2"
config VENDER_PSV
        bool "PSV"
endchoice

source "drivers/media/platform/exynos/fimc-is2/vendor/default/Kconfig"
source "drivers/media/platform/exynos/fimc-is2/vendor/mcd/Kconfig"
source "drivers/media/platform/exynos/fimc-is2/vendor/mcd_v2/Kconfig"
endmenu
